<phpunit
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="https://schema.phpunit.de/6.3/phpunit.xsd"
>

    <testsuites>
      <testsuite name="unit">
        <directory suffix="Test.php">tests</directory>
      </testsuite>
    </testsuites>

    <logging>
        <junit outputFile="build/phpunit/junit.xml"/>
    </logging>

</phpunit>
